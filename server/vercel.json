{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3001",
    "CORS_ORIGINS": "https://your-frontend.vercel.app,http://localhost:5173"
  }
}