{
  "functions": {
    "api/socket.js": {
      "maxDuration": 30
    },
    "api/meetings.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "/api/socket"
    },
    {
      "source": "/api/meetings/(.*)",
      "destination": "/api/meetings"
    },
    {
      "source": "/api/realtime/(.*)",
      "destination": "/api/realtime"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ]
}